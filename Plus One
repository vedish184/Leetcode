class Solution {
    public int[] plusOne(int[] digits) {
        int l=digits.length;
        
        // digits[l-1]=digits[l-1]+1;
        // int res=digits[l-1];
        // return digits;

    for (int i =l- 1; i >= 0; i--) {
	    if (digits[i] < 9) {
		    digits[i]++;
		    return digits;
		
	    }
	    digits[i] = 0;
	
    }

digits = new int[l+ 1];
digits[0] = 1;
return digits;

    }
}
